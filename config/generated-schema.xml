      <reference local="Mahasiswa_id" foreign="id" />
    </foreign-key>
    <column name="KelasParalel_id" type="INTEGER" required="true" primaryKey="true" />
    <foreign-key foreignTable="kelasparalel" onDelete="RESTRICT" onUpdate="RESTRICT">
      <reference local="KelasParalel_id" foreign="id" />
    </foreign-key>
    <column name="created_at" type="TIMESTAMP" />
    <column name="updated_at" type="TIMESTAMP" />
    <index name="fk_Mahasiswa_has_KelasParalel_KelasParalel1_idx">
      <index-column name="KelasParalel_id" />
    </index>
    <index name="fk_Mahasiswa_has_KelasParalel_Mahasiswa1_idx">
      <index-column name="Mahasiswa_id" />
    </index>
  </table>

  <table name="matakuliah" idMethod="native">
    <column name="id" type="INTEGER" required="true" autoIncrement="true" primaryKey="true" />
    <column name="kodeMk" type="VARCHAR" size="45" />
    <column name="nama" type="VARCHAR" size="45" />
    <column name="sks" type="INTEGER" />
    <column name="created_at" type="TIMESTAMP" />
    <column name="updated_at" type="TIMESTAMP" />
    <column name="Jurusan_id" type="INTEGER" required="true" />
    <foreign-key foreignTable="jurusan" onDelete="RESTRICT" onUpdate="RESTRICT">
      <reference local="Jurusan_id" foreign="id" />
    </foreign-key>
    <index name="fk_MataKuliah_Jurusan1_idx">
      <index-column name="Jurusan_id" />
    </index>
  </table>

</database>
